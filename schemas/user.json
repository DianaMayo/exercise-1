{
    "$schema": "https://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "user"
    ],
    "properties": {
        "user": {
            "type": "object",
            "default": {},
            "title": "The user Schema",
            "required": [
                "username",
                "first_name",
                "last_name",
                "email",
                "phone",
                "password"
            ],
            "properties": {
                "username": {
                    "type": "string",
                    "default": "",
                    "title": "The username Schema",
                    "examples": [
                        "DianaM"
                    ]
                },
                "first_name": {
                    "type": "string",
                    "default": "",
                    "title": "The first_name Schema",
                    "examples": [
                        "Diana"
                    ]
                },
                "last_name": {
                    "type": "string",
                    "default": "",
                    "title": "The last_name Schema",
                    "examples": [
                        "Mayo"
                    ]
                },
                "email": {
                    "type": "string",
                    "default": "",
                    "title": "The email Schema",
                    "examples": [
                        "dianamayo25@gmail.com"
                    ]
                },
                "phone": {
                    "type": "string",
                    "default": "",
                    "title": "The phone Schema",
                    "examples": [
                        "3476071806"
                    ]
                },
                "password": {
                    "type": "string",
                    "default": "",
                    "title": "The password Schema",
                    "examples": [
                        "0123456"
                    ]
                },
                "admin": {
                    "type": "boolean",
                    "default": "",
                    "title": "The admin Schema",
                    "examples": [
                        "true"
                    ]
                }
            },
            "examples": [{
                "username": "DianaM",
                "first_name": "Diana",
                "last_name": "Mayo",
                "email": "dianamayo25@gmail.com",
                "phone": "3476071806",
                "password": "0123456",
                "admin": "true"
            }]
        }
    },
    "examples": [{
        "user": {
            "username": "DianaM",
            "first_name": "Diana",
            "last_name": "Mayo",
            "email": "dianamayo25@gmail.com",
            "phone": "3476071806",
            "password": "0123456",
            "admin": "true"
        }
    }]
}